{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "SET NODE_ENV=development npm run clean-and-build && npm run server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "clean-and-build": "npm run clean && npm run build",
    "prod": "SET NODE_ENV=production npm run clean-and-build && npm run server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "nodemon": "^1.19.1"
  }
}
